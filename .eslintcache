ss\";\r\n\r\n//TODO import wegen merkwürdiger Seiteneffekte auf instructions deaktiviert\r\nexport function SessionFinished(props) {\r\n  const {nextSessionStart} = props;\r\n  const t = useContext(LngContext);\r\n  if (!nextSessionStart) {\r\n    return (\r\n      <Html html={t(phrase.sessionFinishedDu)}/>\r\n    );\r\n  }\r\n  return null;\r\n  return (\r\n    <div class={styles.zone}>\r\n      <p> {t(phrase.sessionFinished)} </p>\r\n      <p> {t(phrase.pending)} </p>\r\n      <p> {t(phrase.pendingTime)}\r\n        <span class={styles.time}>\r\n          {timeString((nextSessionStart - Date.now()) / 1000)}\r\n        </span>\r\n      </p>\r\n    </div>\r\n  );\r\n}\r\n\r\nfunction timeString(timeInSeconds) {\r\n  let remainingTimeInSeconds = timeInSeconds;\r\n  const days = Math.floor(remainingTimeInSeconds / 86400);\r\n  remainingTimeInSeconds -= days * 86400;\r\n  const hours = Math.floor(remainingTimeInSeconds / 3600);\r\n  remainingTimeInSeconds -= hours * 3600;\r\n  const minutes = Math.floor(remainingTimeInSeconds / 60);\r\n  remainingTimeInSeconds -= minutes * 60;\r\n  const seconds = Math.floor(remainingTimeInSeconds);\r\n\r\n  if (days > 0) {\r\n    return `${days}d ${hours}h`;\r\n  }\r\n  if (hours > 0) {\r\n    return `${hours}h ${minutes}min`;\r\n  }\r\n  if (minutes > 0) {\r\n    return `${minutes}min ${seconds}s `;\r\n  }\r\n  return `${seconds}s `;\r\n}","G:\\OneDrive_Marienschule\\Marienschule Krefeld\\Programmprojekte - Dokumente\\react\\bipsy\\ebl\\src\\MicroComponents\\Zone.js",[],"G:\\OneDrive_Marienschule\\Marienschule Krefeld\\Programmprojekte - Dokumente\\react\\bipsy\\ebl\\src\\assets\\ressourceLanguage.js",[],"G:\\OneDrive_Marienschule\\Marienschule Krefeld\\Programmprojekte - Dokumente\\react\\bipsy\\ebl\\src\\helper\\TimelineManager.js",[],"G:\\OneDrive_Marienschule\\Marienschule Krefeld\\Programmprojekte - Dokumente\\react\\bipsy\\ebl\\src\\Test\\testTimeline.js",[],"G:\\OneDrive_Marienschule\\Marienschule Krefeld\\Programmprojekte - Do