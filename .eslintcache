 new Promise(resolve => {\r\n      time <= 0 ? resolve() : timeoutArray.current.push(setTimeout(resolve, time));\r\n    });\r\n\r\n    paramArray.map(a => isNaN(a) ? a : promiseTimeout(a))\r\n      .reduce(\r\n        (acc, cur) => {\r\n          return acc.then(cur)\r\n        }, startPromise\r\n      );\r\n  }\r\n\r\n  let startFunction;\r\n\r\n  const startPromise = new Promise((resolve) => startFunction = resolve);\r\n\r\n  return startFunction;\r\n}","G:\\OneDrive_Marienschule\\Marienschule Krefeld\\Programmprojekte - Dokumente\\react\\bipsy\\ebl\\src\\assets\\EBL_01\\examples\\skispringen.js",[],"G:\\OneDrive_Marienschule\\Marienschule Krefeld\\Programmprojekte - Dokumente\\react\\bipsy\\ebl\\src\\assets\\EBL_01\\examples\\explanations.js",[],"G:\\OneDrive_Marienschule\\Marienschule Krefeld\\Programmprojekte - Dokumente\\react\\bipsy\\ebl\\src\\Hooks\\useKeyListenerOnce.js",["258"],"import {useEffect} from \"react\";\r\n\r\nexport function useKeyListenerOnce(myKey, callback, active) {\r\n  useEffect(() => {\r\n    if (active) {\r\n      document.addEventListener('keypress', handleKeyPress);\r\n    }\r\n    return () => document.removeEventListener('keypress', handleKeyPress);\r\n  }, [active]);\r\n\r\n  function handleKeyPress(e) {\r\n    if (active && myKey === e.key) {\r\n      document.removeEventListener('keypress', handleKeyPress);\r\n      callback();\r\n    }\r\n  }\r\n}","G:\\OneDrive_Marienschule\\Marienschule Krefeld\\Programmprojekte - Dokumente\\react\\bipsy\\ebl\\src\\MicroComponents\\FeedbackWrong.js",[],"G:\\OneDrive_Marienschule\\Marienschule Krefeld\\Programmprojekte - Dokumente\\react\\bipsy\\ebl\\src\\MicroComponents\\FeedbackCorrect.js",[],"G:\\OneDrive_Marienschule\\Marienschule Krefeld\\Programmprojekte - Dokumente\\react\\bipsy\\ebl\\src\\MicroComponents\\Video.js",[],"G:\\OneDrive_Marienschule\\Marienschule Krefeld\\Programmprojekte - Dokumente\\react\\bipsy\\ebl\\src\\MicroComponents\\AdvancedInput.js",[],"G:\\OneDrive_Marienschule\\Ma