anguage\";\r\nimport {Html} from \"./Html\";\r\nimport styles from \"../css/SessionFinished.module.css\";\r\n\r\n//TODO import wegen merkwürdiger Seiteneffekte auf instructions deaktiviert\r\nexport function SessionFinished(props) {\r\n  const {nextSessionStart} = props;\r\n  const t = useContext(LngContext);\r\n  if (!nextSessionStart) {\r\n    return (\r\n      <Html html={t(phrase.sessionFinishedDu)}/>\r\n    );\r\n  }\r\n  return null;\r\n  return (\r\n    <div class={styles.zone}>\r\n      <p> {t(phrase.sessionFinished)} </p>\r\n      <p> {t(phrase.pending)} </p>\r\n      <p> {t(phrase.pendingTime)}\r\n        <span class={styles.time}>\r\n          {timeString((nextSessionStart - Date.now()) / 1000)}\r\n        </span>\r\n      </p>\r\n    </div>\r\n  );\r\n}\r\n\r\nfunction timeString(timeInSeconds) {\r\n  let remainingTimeInSeconds = timeInSeconds;\r\n  const days = Math.floor(remainingTimeInSeconds / 86400);\r\n  remainingTimeInSeconds -= days * 86400;\r\n  const hours = Math.floor(remainingTimeInSeconds / 3600);\r\n  remainingTimeInSeconds -= hours * 3600;\r\n  const minutes = Math.floor(remainingTimeInSeconds / 60);\r\n  remainingTimeInSeconds -= minutes * 60;\r\n  const seconds = Math.floor(remainingTimeInSeconds);\r\n\r\n  if (days > 0) {\r\n    return `${days}d ${hours}h`;\r\n  }\r\n  if (hours > 0) {\r\n    return `${hours}h ${minutes}min`;\r\n  }\r\n  if (minutes > 0) {\r\n    return `${minutes}min ${seconds}s `;\r\n  }\r\n  return `${seconds}s `;\r\n}","G:\\OneDrive_Marienschule\\Marienschule Krefeld\\Programmprojekte - Dokumente\\react\\bipsy\\ebl\\src\\MicroComponents\\Zone.js",[],"G:\\OneDrive_Marienschule\\Marienschule Krefeld\\Programmprojekte - Dokumente\\react\\bipsy\\ebl\\src\\assets\\ressourceLanguage.js",[],"G:\\OneDrive_Marienschule\\Marienschule Krefeld\\Programmprojekte - Dokumente\\react\\bipsy\\ebl\\src\\helper\\TimelineManager.js",[],"G:\\OneDrive_Marienschule\\Marienschule Krefeld\\Programmprojekte - Dokumente\\react\\bipsy\\ebl\\src\\Test\\testTimeline.js",[],"G:\\OneDrive_Marienschule\\Marienschule Krefeld\\Programmprojekte - Dokumente\\react\\bipsy\\ebl\\src\\Frames\\EBL\\EblFrame.js",["331"],"import React, {useContext, useRef, useState} from \"react\";\r\nimport {LngContext} from \"../../helper/i18n\";\r\nimport {Html} from \"../../MicroComponents/Html\";\r\nimport styles from \"../../css/EBLFrame.module.css\";\r\nimport {phrase} from \"../../assets/ressourceLanguage\";\r\nimport {Button, Intent, TextArea} from \"@blueprintjs/core\";\r\nimport {ResponseRadioButtons} from \"../../MicroComponents/ResponseRadioButtons\";\r\n\r\nexport function EblFrame(props) {\r\n  const {content, config} = props;\r\n  const t = useContext(LngContext);\r\n  const [activeExp, setActiveExp] = useState(0); // active explanation box\r\n  const logData = useRef({string: content.string, explanations: []});\r\n  const header = content.singleHeader && <Html html={t(content.htmlHeader)}/>;\r\n\r\n  const nextExplanation = (data) => {\r\n    logData.current.explanations.push(data);\r\n    if (activeExp >= explanations.length - 1) {\r\n      props.finish(logData.current);\r\n    } else {\r\n      setActiveExp(activeExp + 1);\r\n    }\r\n  }\r\n\r\n  const textExplanations = content.htmlExplanations.map((explanation, index) =>\r\n    <SingleExplanation\r\n      id={explanation.id}\r\n      key={index}\r\n      explanation={explanation && explanation.html}\r\n      callback={nextExplanation}\r\n      active={activeExp === index}\r\n      minLength={config.textAreaMinLength}\r\n    />\r\n  );\r\n  const radios = content.htmlRadios.map((radio, index) =>\r\n    <SingleRadios\r\n      key={index}\r\n      html={radio.html}\r\n      callback={nextExplanation}\r\n      active={activeExp === index}\r\n      options={radio.options}\r\n      id={radio.id}\r\n    />\r\n  )\r\n\r\n  const explanations = [...textExplanations, ...radios,];\r\n  if (content.button) {\r\n    explanations.push(<ButtonDiv key={0} callback={props.finish}/>);\r\n  }\r\n\r\n\r\n  const activeRadioNumbers=content.htmlRadios[activeExp];\r\n  const activeExplanationNumbers=content.htmlExplanations[activeExp];\r\n\r\n  const activeNrs = [];\r\n  if (activeRadioNumbers && activeRadioNumbers.exampleNrs) {\r\n    activeNrs.push(...activeRadioNumbers.exampleNrs);\r\n  }\r\n  if (activeExplanationNumbers && activeExplanationNumbers.exampleNrs) {\r\n    activeNrs.push(...activeExplanationNumbers.exampleNrs);\r\n  } //TODO eleganter\r\n\r\n  const examples = content.htmlExamples.map((ex, index) => {\r\n      return <SingleExample\r\n        key={index}\r\n        example={ex}\r\n        nr={index + 1}\r\n        active={activeNrs.includes(index)}\r\n        singleHeader={content.singleHeader}\r\n        showCount={content.htmlExamples.length > 1}\r\n      />;\r\n    }\r\n  );\r\n\r\n  return (\r\n    <div className={styles.eblFrame}>\r\n      <div className={styles.exampleContainer}>\r\n        <ExHeader header={header}/>\r\n        <Examples examples={examples}/>\r\n      </div>\r\n      <div className={styles.explanations}>\r\n        {explanations}\r\n      </div>\r\n\r\n    </div>\r\n  )\r\n    ;\r\n}\r\n\r\nfunction ExHeader(props) {\r\n  const\r\n    {header} = props;\r\n  return header ?\r\n    <div\r\n      className={styles.exampleHeader}>\r\n      {header}\r\n    </div> : null;\r\n}\r\n\r\nfunction Examples(props) {\r\n  const {examples} = props;\r\n  return (\r\n    <div className={styles.examples}>\r\n      {examples}\r\n    </div>\r\n  );\r\n}\r\n\r\nfunction SingleExample(props) {\r\n  const t = useContext(LngContext);\r\n  const {example, nr, singleHeader, showCount, active} = props;\r\n\r\n  const activeClass = active ? ' ' + styles.highlight : '';\r\n  return (\r\n    <div className={styles.singleExample + activeClass}>\r\n      <div>\r\n        <div className={styles.exampleCount}>\r\n          {t(phrase.eblQuestion)} {showCount ? nr : ''}:\r\n        </div>\r\n        {singleHeader ? '' : <Html html={t(example.htmlHeader)}/>}\r\n        <Html className={styles.question} html={t(example.htmlProblem)}/>\r\n      </div>\r\n      <div className={styles.solution}>\r\n        <Html html={t(phrase.solution) + ':&nbsp;&nbsp;'}/>\r\n        {example.solution}</div>\r\n    </div>\r\n  );\r\n}\r\n\r\nfunction SingleExplanation(props) {\r\n  const t = useContext(LngContext);\r\n  const {explanation, callback, active, minLength} = props;\r\n  const [text, setText] = useState('');\r\n\r\n  const activeClass = active ? ' ' + styles.highlight : '';\r\n  return (\r\n    <div className={styles.singleExplanation + activeClass}>\r\n      <Html className={styles.explanationHeader} html={t(explanation)}/>\r\n      {active ? <TextArea\r\n        className={styles.editable}\r\n        placeholder={t(phrase.editablePlaceholder)}\r\n        disabled={!active}\r\n        onChange={(ev) => setText(ev.target.value)}\r\n        intent={Intent.PRIMARY}\r\n        rows={9}\r\n        cols={25}\r\n      /> : null}\r\n      {active ? <Button intent={active ? 'primary' : 'none'}\r\n                        disabled={!active || text.length < minLength}\r\n                        onClick={() => callback(text)}>\r\n        {t(phrase.continue)}\r\n      </Button> : null}\r\n    </div>\r\n  );\r\n}\r\n\r\nfunction SingleRadios(props) {\r\n  const t = useContext(LngContext);\r\n  const {callback, options, html, active, id} = props;//TODO id benutzen\r\n  const activeClass = active ? ' ' + styles.highlight : '';\r\n  return (\r\n    <div className={styles.singleExplanation + activeClass}>\r\n      <Html\r\n        className={styles.explanationHeader}\r\n        style={{marginBottom: '8px'}}\r\n        html={t(html)}/>\r\n      {active ? <ResponseRadioButtons\r\n        callback={callback}\r\n        options={options}\r\n        autoContinue={true}\r\n      /> : null}\r\n    </div>\r\n  )\r\n}\r\n\r\nfunction ButtonDiv(props) {\r\n  const t = useContext(LngContext);\r\n  return (\r\n    <div className={styles.buttonDiv}>\r\n      <Button fill intent={'primary'} onClick={props.callback}>\r\n        {t(phrase.continue)}\r\n      </Button>\r\n    </div>\r\n  );\r\n}\r\n","G:\\OneDrive_Marienschule\\Marienschule Krefeld\\Programmprojekte - Dokumente\\react\\bipsy\\ebl\\src\\Frames\\FixationCrossFrame.js",[],"G:\\OneDrive_Marienschule\\Marienschule Krefeld\\Programmprojekte - Dokumente\\react\\bipsy\\ebl\\src\\MicroComponents\\ResponseInput.js",[],"G:\\OneDrive_Marienschule\\Marienschule Krefeld\\Programmprojekte - Dokumente\\react\\bipsy\\ebl\\src\\MicroComponents\\Html.js",[],"G:\\OneDrive_Marienschule\\Marienschule Krefeld\\Programmprojekte - Dokumente\\react\\bipsy\\ebl\\src\\MicroComponents\\ResponseRadioButtons.js",[],"G:\\OneDrive_Marienschule\\Marienschule Krefeld\\Programmprojekte - Dokumente\\react\\bipsy\\ebl\\src\\MicroComponents\\ResponseTextArea.js",[],"G:\\OneDrive_Marienschule\\Marienschule Krefeld\\Programmprojekte - Dokumente\\react\\bipsy\\ebl\\src\\Test\\Test1.js",[],"G:\\OneDrive_Marienschule\\Marienschule Krefeld\\Programmprojekte - Dokumente\\react\\bipsy\\ebl\\src\\Frames\\StimulusResponseFrame.js",[],"G:\\OneDrive_Marienschule\\Marienschule Krefeld\\Programmprojekte - Dokumente\\react\\bipsy\\ebl\\src\\assets\\ressourceIMI.js",[],"G:\\OneDrive_Marienschule\\Marienschule Krefeld\\Programmprojekte - Dokumente\\react\\bipsy\\ebl\\src\\Frames\\EBL\\EBL01Video.js",[],"G:\\OneDrive_Marienschule\\Marienschule Krefeld\\Programmprojekte - Dokumente\\react\\bipsy\\ebl\\src\\MicroComponents\\FlexZone.js",[],"G:\\OneDrive_Marienschule\\Marienschule Krefeld\\Programmprojekte - Dokumente\\react\\bipsy\\ebl\\src\\Components\\Likert.js",[],"G:\\OneDrive_Marienschule\\Marienschule Krefeld\\Programmprojekte - Dokumente\\react\\bipsy\\ebl\\src\\Hooks\\useStateDelayed.js",[],"G:\\OneDrive_Marienschule\\Marienschule Krefeld\\Programmprojekte - Dokumente\\react\\bipsy\\ebl\\src\\Hooks\\useTimeout.js",["332","333"],"import {useEffect, useRef} from \"react\";\r\n\r\nexport function useTimeout(...paramArray) {\r\n  const timeoutArray = useRef([]);\r\n  useEffect(() => {\r\n    buildTimeouts();\r\n    return () => {\r\n      timeoutArray.current.forEach(to => clearTimeout(to));\r\n    }\r\n  }, []);\r\n\r\n  function buildTimeouts() {\r\n    const promiseTimeout = time => () => new Promise(resolve => {\r\n      time <= 0 ? resolve() : timeoutArray.current.push(setTimeout(resolve, time));\r\n    });\r\n\r\n    paramArray.map(a => isNaN(a) ? a : promiseTimeout(a))\r\n      .reduce(\r\n        (acc, cur) => {\r\n          return acc.then(cur)\r\n        }, startPromise\r\n      );\r\n  }\r\n\r\n  let startFunction;\r\n\r\n  const startPromise = new Promise((resolve) => startFunction = resolve);\r\n\r\n  return startFunction;\r\n}","G:\\OneDrive_Marienschule\\Marienschule Krefeld\\Programmprojekte - Dokumente\\react\\bipsy\\ebl\\src\\Hooks\\useKeyListenerOnce.js",["334"],"import {useEffect} from \"react\";\r\n\r\nexport function useKeyListenerOnce(myKey, callback, active) {\r\n  useEffect(() => {\r\n    if (active) {\r\n      document.addEventListener('keypress', handleKeyPress);\r\n    }\r\n    return () => document.removeEventListener('keypress', handleKeyPress);\r\n  }, [active]);\r\n\r\n  function handleKeyPress(e) {\r\n    if (active && myKey === e.key) {\r\n      document.removeEventListener('keypress', handleKeyPress);\r\n      callback();\r\n    }\r\n  }\r\n}","G:\\OneDrive_Marienschule\\Marienschule Krefeld\\Programmprojekte - Dokumente\\react\\bipsy\\ebl\\src\\MicroComponents\\FeedbackWrong.js",[],"G:\\OneDrive_Marienschule\\Marienschule Krefeld\\Programmprojekte - Dokumente\\react\\bipsy\\ebl\\src\\MicroComponents\\FeedbackCorrect.js",[],"G:\\OneDrive_Marienschule\\Marienschule Krefeld\\Programmprojekte - Dokumente\\react\\bipsy\\ebl\\src\\MicroComponents\\Video.js",[],"G:\\OneDrive_Marienschule\\Marienschule Krefeld\\Programmprojekte - Dokumente\\react\\bipsy\\ebl\\src\\MicroComponents\\AdvancedInput.js",[],"G:\\OneDrive_Marienschule